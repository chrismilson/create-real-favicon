{
  "name": "create-real-favicon",
  "version": "0.2.1",
  "description": "Node.js CLI for RealFaviconGenerator",
  "bin": "lib/index.js",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src/**/* --ext .js,.jsx,.ts,.tsx",
    "preversion": "yarn lint",
    "version": "yarn lint --fix && git add -A src",
    "postversion": "git push && git push --tags",
    "prepare": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrismilson/create-real-favicon.git"
  },
  "keywords": [
    "favicon",
    "cli",
    "RealFaviconGenerator"
  ],
  "author": {
    "name": "Chris Milson",
    "email": "chris.w.milson@gmail.com"
  },
  "license": "MIT",
  "preferGlobal": true,
  "bugs": {
    "url": "https://github.com/chrismilson/create-real-favicon/issues"
  },
  "homepage": "https://github.com/chrismilson/create-real-favicon#readme",
  "dependencies": {
    "commander": "^2.20.0",
    "rfg-api": "^0.5.1"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rollup": "^1.32.0",
    "rollup-plugin-hashbang": "^2.2.2",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.2"
  }
}
